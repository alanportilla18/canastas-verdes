# Base de datos PostgreSQL
# Para desarrollo local, ajusta los valores según tu configuración
DATABASE_URL="postgresql://postgres:oscar@localhost:5432/canastas_verdes?schema=public"

# Secretos para JWT (CAMBIAR EN PRODUCCIÓN)
ACCESS_TOKEN_SECRET="tu_secreto_super_seguro_para_access_token"
REFRESH_TOKEN_SECRET="tu_secreto_super_seguro_para_refresh_token"

# Tiempo de expiración de tokens (en segundos)
ACCESS_TOKEN_EXPIRES_IN=3600
REFRESH_TOKEN_EXPIRES_IN=86400

